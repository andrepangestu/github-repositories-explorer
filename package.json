{
  "name": "github-repositories-explorer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A modern React + TypeScript application for exploring GitHub users and their repositories with real-time search and comprehensive repository details",
  "keywords": [
    "github",
    "repositories",
    "react",
    "typescript",
    "vite",
    "tailwindcss",
    "api",
    "search",
    "explorer"
  ],
  "author": {
    "name": "Andre Pangestu",
    "email": "andre.pangestu22@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrepangestu/github-repositories-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/andrepangestu/github-repositories-explorer/issues"
  },
  "homepage": "https://andrepangestu.github.io/github-repositories-explorer",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist node_modules/.vite",
    "prebuild": "npm run clean",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ci": "vitest run --reporter=verbose",
    "test:coverage": "vitest run --coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
